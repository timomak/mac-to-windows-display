<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Allow unsigned executable memory (needed for JIT in some cases) -->
    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <true/>
    
    <!-- Disable library validation (for loading system frameworks) -->
    <key>com.apple.security.cs.disable-library-validation</key>
    <true/>
    
    <!-- Network client capability (for QUIC connections) -->
    <key>com.apple.security.network.client</key>
    <true/>
    
    <!-- Network server capability (if needed for listening) -->
    <key>com.apple.security.network.server</key>
    <true/>
</dict>
</plist>

